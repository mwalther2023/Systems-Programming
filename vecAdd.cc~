#include <iostream>

int main(){
  int n = 100;
  int* a = (int*)malloc(n*sizeof(int));
  int* b = (int*)malloc(n*sizeof(int));
  int* c = (int*)malloc(n*sizeof(int));

  for(int i = 0; i<n; i++){
    a[i] = i;
    b[i] = i;
  }

  //Vec Add
  for(int i = 0; i<n; i++){
    c[i] = a[i] + b[i];
  }

  for(int i = 0; i<n; i++){
    cout << "value " <<c[i];
	   
  }
}
